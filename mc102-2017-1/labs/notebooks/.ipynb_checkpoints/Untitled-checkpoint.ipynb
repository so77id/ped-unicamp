    "def print_mat(map):\n",
    "    for i in range(len(map)):\n",
    "        print(\" \".join(map[i]))\n",
    "\n",
    "inp = input()\n",
    "mat = []\n",
    "\n",
    "while not inp.isdigit():\n",
    "    l = inp.split()\n",
    "    mat.append(l)\n",
    "    inp = input()\n",
    "\n",
    "print_mat(mat)\n"
   ]
